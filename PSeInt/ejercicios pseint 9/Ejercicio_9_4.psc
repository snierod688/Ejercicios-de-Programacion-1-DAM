Algoritmo Ejercicio_9_4
	Dimension v1[3]
	Dimension v2[3]
	Escribir "Introduce los valores del vector x"
	Para i<-1 Hasta 3 Hacer
		Escribir Sin Saltar "x"
		Leer v1[i]
	FinPara
	Escribir "Introduce los valores del vector y"
	Para i<-1 Hasta 3 Hacer
		Escribir Sin Saltar "y"
		Leer v2[i]
	FinPara
	Escribir "La primera suma es: x" v1[1] "+ y" v2[1]
	Escribir "La segunda suma es: x" v1[2] "+ y" v2[2]
	Escribir "La tercera suma es: x" v1[3] "+ y" v2[3]
FinAlgoritmo
